<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MascotasFavoritas">

    <include android:id="@+id/miActionBarRated"
        layout="@layout/actionbar2">
    </include>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rvMascotasFavs"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/miActionBarRated"
        android:paddingLeft="@dimen/RV_padding_left"
        android:paddingRight="@dimen/RV_padding_right">

    </android.support.v7.widget.RecyclerView>

</RelativeLayout>